        "jest-fetch-mock": "3.0.3",
        "kutty": "0.6.0",
        "lint-staged": "12.4.1",
        "msw": "0.39.2",
        "postcss": "8.4.13",
        "prettier": "2.6.2",
        "prisma": "3.13.0",
        "sort-package-json": "1.57.0",
        "tailwindcss": "3.0.24",
        "typescript": "4.6.4"
    },
    "msw": {
        "workerDirectory": "public"
    }
