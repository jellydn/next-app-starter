{
    "name": "next-app-starter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "build-storybook": "build-storybook",
        "dev": "next dev",
        "lint": "next lint",
        "prepare": "husky install",
        "start": "next start",
        "storybook": "start-storybook -p 6006",
        "test": "jest"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --cache --fix",
            "prettier --write"
